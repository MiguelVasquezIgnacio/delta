@model IEnumerable<MiRadio_Startup.Models.Musica>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaPublicacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Titulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Autor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TamañoMB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrlMusica)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaPublicacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Titulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Autor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TamañoMB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrlMusica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMusica">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMusica">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMusica">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
