@model IEnumerable<MiRadio_Startup.Models.Comentario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Texto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaPublicacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MusicaS)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Texto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaPublicacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioS.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MusicaS.Autor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdComentario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdComentario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdComentario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
